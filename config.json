{
    "outputName" : "sampleBuildFileName",
    "concat" : {
        "enabled" : true,
        "order" : ["src/sample1.js","src/sample2.js"]
    },
    "expand" : {
        "enabled" : true,
        "prefix" : "<% ",
        "suffix" : " %>"
    },
    "beautifier" : {
        "enabled" : true,
        "indentChar" : " ",
        "indentSize" : 2,
        "eol" : "\n",
        "endWithNewline" : true,
        "convertEncoding" : "UTF-8"
    },
    "minify" : {
        "enabled" : true
    },
    "jshint" : {
        "enabled" : true,
        "rcfile" : ".jshintrc"
    },
    "mochaTest" : {
        "enabled" : true
    },
    "jsdoc3" : {
        "enabled" : true
    },
    "browserSync" : {
        "enabled" : true,
        "open" : false,
        "host" : "",
        "port" : 60000,
        "logLevel" : "info",
        "logConnections" : true,
        "reloadOnRestart" : true,
        "port" : 10000,
        "server" : {
          "baseDir" : "./",
          "indexFile" : "index.html"
        },
        "ui" : {
          "port" : 10001
        }
    }
